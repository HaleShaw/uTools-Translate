{
  "pluginName": "翻译",
  "version": "3.1.0",
  "description": "中英翻译插件，支持多API切换、音标朗读、句子朗读、实时输入翻译、原生列表自动复制",
  "author": "HaleShaw",
  "homepage": "https://github.com/HaleShaw/uTools-Translate",
  "main": "index.html",
  "preload": "preload.js",
  "logo": "logo.png",
  "features": [
    {
      "code": "translate_text",
      "explain": "中英文翻译，音标朗读，句子朗读，多API切换，自动复制。",
      "cmds": [
        "翻译",
        "translate",
        "tr",
        {
          "type": "over",
          "label": "翻译",
          "maxLength": 5000
        }
      ]
    },
    {
      "code": "translate_setting",
      "explain": "翻译设置，切换翻译API",
      "cmds": ["翻译设置", "translate setting", "setting", "t setting", "设置", "ts"]
    }
  ]
}
