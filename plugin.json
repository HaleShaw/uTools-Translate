{
  "pluginName": "翻译",
  "version": "3.0.1",
  "description": "中英翻译插件，支持多API切换、音标句子朗读、实时输入翻译、原生列表自动复制",
  "author": "HaleShaw",
  "homepage": "https://github.com/HaleShaw/uTools-Translate",
  "main": "index.html",
  "logo": "logo.png",
  "features": [
    {
      "code": "translate_text",
      "explain": "中英文翻译，音标朗读，句子朗读，多API切换。",
      "cmds": [
        "翻译",
        "translate",
        "tr",
        {
          "type": "regex",
          "label": "翻译",
          "match": "/^[\u4e00-\u9fa5a-zA-Z][\u4e00-\u9fa5a-zA-Z0-9' ]/",
          "minLength": 1
        }
      ]
    },
    {
      "code": "translate_setting",
      "explain": "翻译设置，切换翻译API",
      "cmds": ["翻译设置", "translate setting", "setting", "t setting", "设置", "ts"]
    }
  ]
}
